# This file defines the standard default settings at Jeppesen, as distinct
# from TextTest's own default settings.

# plenty of people use CVS at Jeppesen, best to ignore it in data
test_data_ignore:CVS

# roughly number of R&D Linux machines, with a bit extra for luck
queue_system_max_capacity:70 

[bug_system_location]
bugzilla:http://bugzilla.carmen.se

# These session names "count" as a nightjob and use the "batch_version" setting to decide what is run
[batch_use_version_filtering]
nightjob:true
wkendjob:true
release*:true

# How old should we allow collected results to be?
[batch_collect_max_age_days]
nightjob:1
wkendjob:3
